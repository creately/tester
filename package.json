{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "A bare minimum starter repository for writing and testing browser modules using typescript.",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"src/**/*.ts\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "prepare": "npm run lint && npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "bin": {
    "tester": "./dist/tester.js"
  },
  "preferGlobal": true,
  "author": "Anonymous",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "4.14.149",
    "@types/puppeteer": "^2.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fs": "0.0.1-security",
    "globby": "^10.0.1",
    "image-diff": "^2.0.0",
    "imagemagick": "^0.1.3",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "puppeteer": "^2.0.0",
    "yargs": "^15.0.2"
  },
  "files": [
    "dist/**/*"
  ]
}
